import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner se = new Scanner(System.in);
        int n = se.nextInt();
        int a = n/10;
        int b = n%10;
        int s = a +b;
        System.out.println("Bird said:"+n);
        System.out.println("Alice must go in path-"+s);
    }
}
