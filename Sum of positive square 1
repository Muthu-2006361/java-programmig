import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner se=new Scanner(System.in);
        int a=se.nextInt();
        int arr[]=new int[a];
        int sum=0;
        for(int i=0;i<a;i++){
            arr[i]=se.nextInt();
            if(arr[i]>0){
                sum=sum+arr[i]*arr[i];
            }
        }
        System.out.println(sum);
        
    }
}
