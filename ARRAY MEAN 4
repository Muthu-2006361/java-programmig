import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner se=new Scanner(System.in);
        int a=se.nextInt();
        int arr[]=new int[a];
        float sum=0;
        for(int i=0;i<a;i++){
            arr[i]=se.nextInt();
            sum=sum+arr[i];
            
        }
        System.out.printf("Array Mean Value is %.2f",sum/a);
    }
}
